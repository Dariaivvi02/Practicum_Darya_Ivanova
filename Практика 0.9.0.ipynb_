}},{"cell_type":"code","source":["import requests\n","import random\n","\n","team = []\n","def add_pokemon(name):\n","    if find_pokemon(name):\n","        print(f\"{name.capitalize()} уже в вашей команде!\")\n","        return\n","\n","    url = f\"https://pokeapi.co/api/v2/pokemon/{name.lower()}\"\n","    response = requests.get(url)\n","\n","    if response.status_code == 200:\n","        pokemon_data = response.json()\n","        pokemon = {\n","            \"name\": pokemon_data[\"name\"],\n","            \"health\": pokemon_data[\"stats\"][0][\"base_stat\"],\n","            \"attack\": pokemon_data[\"stats\"][1][\"base_stat\"],\n","            \"defense\": pokemon_data[\"stats\"][2][\"base_stat\"],\n","            \"type\": pokemon_data[\"types\"][0][\"type\"][\"name\"],\n","            \"sprite\": pokemon_data[\"sprites\"][\"front_default\"],\n","        }\n","        team.append(pokemon)\n","        print(f\"{pokemon['name'].capitalize()} добавлен в вашу команду\")\n","    else:\n","        print(\"покемон не найден, проверьте имя\")\n","\n","\n","def remove_pokemon(name):\n","    pokemon = find_pokemon(name)\n","    if pokemon:\n","        team.remove(pokemon)\n","        print(f\"{name.capitalize()} удален из команды\")\n","    else:\n","        print(f\"{name.capitalize()} не найден в вашей команде\")\n","\n","\n","def view_team():\n","    if not team:\n","        print(\"Ваша команда пуста\")\n","        return\n","    for pokemon in team:\n","        print(f\"Имя: {pokemon['name'].capitalize()}, Здоровье: {pokemon['health']}, Атака: {pokemon['attack']}, Защита: {pokemon['defense']}, Тип: {pokemon['type']}\")\n","        print(f\"Сприт: {pokemon['sprite']}\\n\")\n","\n","\n","def find_pokemon(name):\n","    for pokemon in team:\n","        if pokemon[\"name\"] == name.lower():\n","            return pokemon\n","    return None\n","\n","\n","def main():\n","    while True:\n","        print(\"\\n1. Добавить покемона\")\n","        print(\"2. Удалить покемона\")\n","        print(\"3. Просмотреть команду\")\n","        print(\"4. Найти покемона\")\n","        print(\"5. Устроить бой\")\n","        print(\"6. Выход\")\n","        choice = input(\"Выберите действие: \")\n","\n","        if choice == '1':\n","            name = input(\"Введите имя покемона: \")\n","            add_pokemon(name)\n","        elif choice == '2':\n","            name = input(\"Введите имя покемона для удаления: \")\n","            remove_pokemon(name)\n","        elif choice == '3':\n","            view_team()\n","        elif choice == '4':\n","            name = input(\"Введите имя покемона для поиска: \")\n","            pokemon = find_pokemon(name)\n","            if pokemon:\n","                print(f\"Имя: {pokemon['name'].capitalize()}, Здоровье: {pokemon['health']}, Атака: {pokemon['attack']}, Защита: {pokemon['defense']}, Тип: {pokemon['type']}\")\n","                print(f\"Сприт: {pokemon['sprite']}\")\n","            else:\n","                print(f\"{name.capitalize()} не найден в вашей команде.\")\n","        elif choice == '5':\n","            name1 = input(\"Введите имя первого покемона: \")\n","            name2 = input(\"Введите имя второго покемона: \")\n","\n","        elif choice == '6':\n","            print(\"Выход из программы\")\n","            break\n","        else:\n","            print(\"Неверный выбор. Пожалуйста, попробуйте снова\")\n","\n","if __name__ == \"__main__\":\n","    main()"],"metadata":{"id":"NnGYp1MTqi5k","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1731498980891,"user_tz":-300,"elapsed":22102,"user":{"displayName":"Дарья Иванова","userId":"14793641154921052260"}},"outputId":"31f2beca-67b7-4711-e2c9-24fb6350976a"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","1. Добавить покемона\n","2. Удалить покемона\n","3. Просмотреть команду\n","4. Найти покемона\n","5. Устроить бой\n","6. Выход\n","Выберите действие: 1